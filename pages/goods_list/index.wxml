            url="/pages/goods_detail/index?goods_id={{item.goods_id}}"
            >
              <view class="goodList_item_l">
                <image mode="widthFix" src="{{item.goods_small_logo||'http://img2.imgtn.bdimg.com/it/u=1739624281,530295853&fm=26&gp=0.jpg'}}"></image>
              </view>
